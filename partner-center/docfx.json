{
  "build": {
    "content": [
       {
        "files": ["breadcrumb/toc.yml"]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "bilingual_type": "hover over",
      "uhfHeaderId": "MSDocsHeader-Partner-Center",
      "titleSuffix": "Partner Center",
      "breadcrumb_path": "/partner-center/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "ms.service": "partner-dashboard",
      "manager": "jasonh",
      "ms.localizationpriority": "medium",
      "recommendations": true,
      "searchScope": [ "Partner Center" ],
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "MSDN.partner-center",
          "folder_relative_path_in_docset": "./"
        }
      },
      "disable_contribution_on_translation_quality": [
        "MT",
        "Blank"
      ]
    },
    "fileMetadata": {
      "recommendations": {
       "announcements/*.md": false 
      }
    },
    "template": [],
    "dest": "partner-center",
    "markdownEngineName": "markdig"
  }
}
